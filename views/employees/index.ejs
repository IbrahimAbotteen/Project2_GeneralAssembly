<%- include ('../partials/boilerplate') %>
    <div class="pageTitle">
        <a href="/emp/add">Add New Employee</a>
        <a class="editLink" href="/main">Home</a>
        <a class="editLink" href="auth/logout">logout</a>
        

        <h2>My Employees:</h2>
       <% for(i of data.emp){ %>
         <div class="data">
             <h3><%=i.name%></h3>
             <a class="infoLink" href="/emp/<%=i.id%>">More Info</a>
             <a class="editLink" href="/emp/<%=i.id%>/edit">Edit</a>
             <form method="POST" action="/emp/<%=i.id%>?_method=DELETE">
                <input type="submit" value="Delete">
            </form>
         </div>
         <% } %>

    </div>
    <%- include ('../partials/end') %>