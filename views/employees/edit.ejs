<%- include ('../partials/boilerplate') %>
   <form class="fill" action="/emp/<%=emp.id%>?_method=PUT" method="POST">
        <input type="text" name="name" value="<%= emp.name %>" placeholder="Name" />
        <input type="text" name="age" value="<%= emp.age %>" placeholder="Age"/>
        <input type="text" name="title" value="<%= emp.title %>" placeholder="Job Title"/>
        <input type="text" name="department" placeholder="Department">
        <input type="submit" value="Update"/>
</form> 
<%- include ('../partials/end') %>